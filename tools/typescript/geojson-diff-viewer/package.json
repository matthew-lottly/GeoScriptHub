{
  "name": "@geoscripthub/geojson-diff-viewer",
  "version": "1.0.0",
  "description": "A Leaflet-based widget for visualising the diff between two GeoJSON FeatureCollections.",
  "type": "module",
  "main": "dist/geojson-diff-viewer.umd.cjs",
  "module": "dist/geojson-diff-viewer.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/geojson-diff-viewer.js",
      "require": "./dist/geojson-diff-viewer.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "dev": "vite build --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src tests",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "geojson",
    "diff",
    "gis",
    "leaflet",
    "geoscripthub"
  ],
  "license": "MIT",
  "peerDependencies": {
    "leaflet": ">=1.9.0",
    "@types/leaflet": ">=1.9.0"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.14",
    "@types/leaflet": "^1.9.14",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "jsdom": "^25.0.0",
    "leaflet": "^1.9.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^2.0.0"
  }
}
